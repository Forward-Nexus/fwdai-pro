{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@fwd-ai/pro",
  "version": "2.3.3",
  "description": "FWD PRO - Better conversations lead to better outcomes. Universal AI expert collaboration system with verified research, natural expert discussion, and organized collaboration.",
  "type": "module",
  "main": "pro-os/system/installer/index.js",
  "bin": {
    "fwdpro": "./bin/cli.js"
  },
  "scripts": {
    "test": "echo \"Tests coming soon\" && exit 0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "husky || true",
    "install-local": "node pro-os/system/installer/index.js",
    "dev": "node pro-os/system/installer/index.js",
    "tools:commit": "node pro-os/system/tools/git-utils/commit-helper.js",
    "tools:pr": "node pro-os/system/tools/git-utils/pr-generator.js",
    "tools:changelog": "node pro-os/system/tools/git-utils/changelog.js",
    "tools:test": "node pro-os/system/tools/testing-utils/test-runner.js",
    "tools:quality": "node pro-os/system/tools/testing-utils/quality-gate.js",
    "tools:env-check": "node pro-os/system/tools/env-utils/env-validator.js",
    "tools:env-gen": "node pro-os/system/tools/env-utils/env-generator.js",
    "tools:secrets": "node pro-os/system/tools/env-utils/secret-scanner.js",
    "tools:migrate": "node pro-os/system/tools/database-utils/migrate.js",
    "tools:seed": "node pro-os/system/tools/database-utils/seed.js",
    "tools:backup": "node pro-os/system/tools/database-utils/backup.js",
    "tools:webhook": "node pro-os/system/tools/api-utils/webhook-tester.js",
    "tools:api-gen": "node pro-os/system/tools/api-utils/api-client-generator.js",
    "tools:vercel": "node pro-os/system/tools/deployment-utils/vercel-setup.js",
    "tools:docker": "node pro-os/system/tools/deployment-utils/docker-setup.js",
    "tools:cicd": "node pro-os/system/tools/deployment-utils/cicd-generator.js",
    "tools:sentry": "node pro-os/system/tools/monitoring-utils/sentry-setup.js",
    "tools:posthog": "node pro-os/system/tools/monitoring-utils/posthog-setup.js",
    "tools:perf": "node pro-os/system/tools/monitoring-utils/performance-setup.js"
  },
  "keywords": [
    "ai",
    "collaboration",
    "founder",
    "startup",
    "business",
    "experts",
    "assistant",
    "roundtable"
  ],
  "author": "William Prejean <info@fwdnxs.com> (ArcCar LLC dba ForwardNexus)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Forward-Nexus/fwdai-pro.git"
  },
  "bugs": {
    "url": "https://github.com/Forward-Nexus/fwdai-pro/issues"
  },
  "homepage": "https://github.com/Forward-Nexus/fwdai-pro",
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@10.20.0",
  "files": [
    "bin/",
    "pro-os/",
    ".cursorignore",
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "CONTRIBUTING.md",
    "ACKNOWLEDGMENTS.md"
  ],
  "dependencies": {
    "inquirer": "^9.2.12",
    "chalk": "^5.3.0",
    "boxen": "^7.1.1",
    "ora": "^7.0.1",
    "fs-extra": "^11.2.0",
    "js-yaml": "^4.1.0",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "mammoth": "^1.8.0",
    "docx": "^8.5.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "husky": "^9.0.0",
    "lint-staged": "^16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "**/*.{js,mjs}": [
      "eslint --fix --max-warnings=0",
      "prettier --write"
    ],
    "**/*.{json,md,yaml}": [
      "prettier --write"
    ]
  }
}
